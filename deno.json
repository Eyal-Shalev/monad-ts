{
	"fmt": {
		"lineWidth": 120,
		"useTabs": true
	},
	"compilerOptions": {},
	"tasks": {
		"fmt": "deno fmt",
		"fmt-check": "deno fmt --check",
		"lint": "deno lint",
		"test": "deno test --check=local --shuffle --doc --parallel",
		"test-lock": "deno task test --lock-write --no-run --reload",
		"test-watch": "deno task test --watch"
	},
	"importMap": "import_map.json"
}
